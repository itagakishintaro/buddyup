{
    "database": {
        "rules": "database.rules.json"
    },
    "hosting": {
        "public": "build/es5-bundled/",
        "ignore": [
            "firebase.json",
            "**/.*"
        ],
        "rewrites": [{
            "source": "!/__/**",
            "destination": "/index.html"
        }]
    }
}
